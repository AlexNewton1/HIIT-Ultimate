<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <size android:height="@dimen/half_fab_size" android:width="@dimen/half_fab_size"/>
    <solid android:color="@android:color/holo_green_dark"/>
    <corners
        android:bottomRightRadius="@dimen/double_fab_size_padded"
        android:bottomLeftRadius="@dimen/half_fab_size_padded"
        android:topLeftRadius="@dimen/fab_size_padded"
        android:topRightRadius="@dimen/half_fab_size_padded"/>
</shape>
